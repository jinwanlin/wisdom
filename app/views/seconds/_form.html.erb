<%= simple_form_for(@second) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :owner %>
    <%= f.input :price %>
    <%= f.input :views %>
    <%= f.input :rate %>
    <%= f.input :desc %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
