<%= simple_form_for(@merchant) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :app_type %>
    <%= f.input :service_type %>
    <%= f.input :name %>
    <%= f.input :title %>
    <%= f.input :contact %>
    <%= f.input :position %>
    <%= f.input :location %>
    <%= f.input :desc %>
    <%= f.input :v_type %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
